<shape aspect="fixed" w="20" h="20" name="Diode" strokewidth="inherit">
  <connections>
    <constraint name="in" perimeter="0" x="0" y="0.5"/>
    <constraint name="out" perimeter="0" x="1" y="0.5"/>
  </connections>
  <background>
  </background>
  <foreground>
    <linejoin join="round"/>
    <linecap cap="round"/>

    <path>
      <move x="0" y="0"/>
      <line x="20" y="10"/>
      <line x="0" y="20"/>
      <close/>
    </path>
    <fillstroke/>

    <path>
      <move x="20" y="0"/>
      <line x="20" y="20"/>
    </path>
    <stroke/>

  </foreground>
</shape>
